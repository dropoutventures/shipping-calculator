{
    "name": "shiptor/shipping-calculator",
    "license": "MIT",
    "type": "library",
    "description": "Package delivery calculation library based on Symfony 2 components.",
    "autoload": {
        "psr-4": {
            "EsteIt\\ShippingCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EsteIt\\ShippingCalculator\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4",
        "symfony/event-dispatcher": "~2.7",
        "symfony/config": "~2.7",
        "moriony/trivial": "~0.1"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0",
        "nelmio/alice": "~2.1"
    },
    "config": {
        "bin-dir": "bin"
    }
}
